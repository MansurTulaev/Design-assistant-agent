# LLM провайдер - Ollama (локальный)
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_MODEL=llama3.2

# Режим запуска
RUN_MODE=both

# RAG настройки (используем локальные эмбеддинги)
USE_LOCAL_EMBEDDINGS=true
LOCAL_EMBEDDING_MODEL=all-MiniLM-L6-v2

# Qdrant (опционально)
QDRANT_API_KEY=

# Redis кэширование
REDIS_CACHE_ENABLED=true

# Docker окружение
DOCKER_ENV=true
